	<queryString>
		<![CDATA[SELECT faktura.broj_fakture AS ID,
	proizvod.naziv AS sID,
	stavka_fakture.kolicina AS sKolicina,
	stavka_fakture.jedinicna_cena AS sJedinicnaCena,
	stavka_fakture.ukupan_iznos AS sUkupanIznos,
	stavka_fakture.stopapdv AS sStopaPDV,
	stavka_fakture.kolicina AS sKolicina,
	stavka_fakture.osnovica AS sOsnovica,
	stavka_fakture.rabat AS sRabat
FROM faktura
	INNER JOIN stavka_fakture ON 
	 stavka_fakture.faktura_id = faktura.id 
	INNER JOIN proizvod ON 
	 proizvod.id = stavka_fakture.proizvod_id 
WHERE 
	 faktura.id = $P{id}]]>
